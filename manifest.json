{
  "name": "INJECT HTML",
  "version": "0.01",
  "description": "hello",
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "open": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "open tab picker"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "fuzzy.js"
      ]
    }
  ],
  "permissions": [
    "scripting",
    "tabs",
    "history"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "manifest_version": 3
}